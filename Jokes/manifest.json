{
    "name": "Random Jokes",
    "version": "1.0",
    "description": "Get a random joke every time you click the extension icon.",
    "manifest_version": 2,
    "icons": {
        "128": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab"
    ]
}
